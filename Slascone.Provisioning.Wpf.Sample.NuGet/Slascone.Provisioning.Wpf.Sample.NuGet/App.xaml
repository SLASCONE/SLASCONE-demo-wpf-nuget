             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="/Main/MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="Logo" UriSource="/Resources/DB-SL-Logo-forcircles256px.png" />
